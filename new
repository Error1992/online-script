function CLOSE()
gg.alert(os.date([[

┏━┳┳┳━┳┳┓ 
┃━┫┃┃┏┫━┫┏┓ 
┃┏┫┃┃┗┫┃┃┃┃ 
┗┛┗━┻━┻┻┛┃┃ 
┏┳┳━┳┳┳┓┏┫┣┳┓ 
┃┃┃┃┃┃┃┃┣┻┫┃┃ 
┣┓┃┃┃┃┣┫┃┏┻┻┫ 
┗━┻━┻━┻❢

🕟Time: %H:%M:%S%p
📅Date: %A, %d %B %Y]]))
print(os.date([[

------------------------------------------------------------------------------------------------------------
𝙲𝙷𝙰𝚁𝚂𝙸 𝚂𝙲𝚁𝙸𝙿𝚃 𝙰𝙻𝙻 𝚅𝙴𝚁𝚂𝙸𝙾𝙽
-----------------------------------------------------------------------------------------------------------
𝙳𝙾𝙽𝚃 𝙵𝙾𝚁𝙶𝙾𝚃 𝚃𝙾 𝙹𝙾𝙸𝙽 𝙼𝚈 𝙲𝙷𝙰𝙽𝙽𝙴𝙻 @𝙲𝙷𝙰𝚁𝚂𝙸𝙶𝙰𝙼𝙸𝙽𝙶_𝙾𝙵𝙵𝙸𝙲𝙸𝙰𝙻 [𝙸𝙽 𝚃𝙴𝙻𝙴𝙶𝚁𝙰𝙼] 
------------------------------------------------------------------------------------------------------------
------------------------------------------------------------------------------------------------------------

🕟Time: %H:%M:%S%p
📅Date: %A, %d %B %Y]]))
gg.skipRestoreState()
  gg.setVisible(true)
  os.exit()
end
while true do
  if gg.isVisible(true) then
PUBGMH = 1
gg.setVisible(false)
  end
if PUBGMH == 1 and CMENU == 0 then
HOME()
  end
  if PUBGMH == 1 and CMENU == 1 then
HOME1()
  end
  if PUBGMH == 1 and CMENU == 2 then
HOME2()
  end
end
