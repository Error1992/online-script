gg.clearResults()
gg.setRanges(gg.REGION_VIDEO)
gg.searchNumber('4.88840463e21;-3.12839322e-39;5.09131994e21::12', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber('-3.12839322e-39', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
